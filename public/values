      window.onkeydown = (e) => {
          if (e.ctrlKey || e.altKey || e.shiftKey || e.metaKey) {
              return;
          }
          
          if (e.key == "ArrowLeft") {
              window.location.href = 'hello-world';
          }
          
          
          if (e.key == "ArrowRight") {
              window.location.href = 'variables';
          }
          
      }
  </script>
